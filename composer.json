{
  "description": "Send SMS messages to your Free Mobile device. A simple and free way to monitor your applications using a Free Mobile account.",
  "homepage": "https://dev.belin.io/free-mobile.php",
  "license": "MIT",
  "name": "cedx/free-mobile",
  "type": "library",
  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "psr-4": {"FreeMobile\\": "lib/"}
  },
  "autoload-dev": {
    "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"],
    "psr-4": {"FreeMobile\\": "test/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "api",
    "client",
    "free",
    "mobile",
    "sdk",
    "sms"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "evenement/evenement": "^3.0.1",
    "guzzlehttp/guzzle": "^6.3.3"
  },
  "require-dev": {
    "cedx/coveralls": "^8.0.0",
    "phing/phing": "^2.16.1",
    "phpstan/phpstan": "^0.10.3",
    "phpunit/phpunit": "^7.3.1"
  },
  "scripts": {
    "coverage": "coveralls var/coverage.xml",
    "test": "phpunit"
  },
  "support": {
    "docs": "https://dev.belin.io/free-mobile.php/api",
    "issues": "https://github.com/cedx/free-mobile.php/issues"
  }
}
