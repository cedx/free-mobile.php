{
	"description": "Send SMS messages to your Free Mobile device.",
	"homepage": "https://github.com/cedx/free-mobile.php",
	"license": "MIT",
	"name": "cedx/free-mobile",
	"type": "library",
	"version": "18.1.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"psr-4": {"freemobile\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"freemobile\\": "test/"}
	},
	"config": {
		"platform-check": false
	},
	"keywords": [
		"api",
		"client",
		"free",
		"mobile",
		"sms"
	],
	"require": {
		"php": ">=8.3.0",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"nyholm/psr7": "^1.8.1",
		"psr/http-client": "^1.0.3",
		"symfony/http-client": "^7.0.3"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10.59",
		"phpunit/phpunit": "^11.0.3"
	},
	"scripts": {
		"clean": "@php tool/clean.php",
		"doc": "@php tool/doc.php",
		"lint": "phpstan analyse --configuration=etc/phpstan.php --memory-limit=256M",
		"publish": "@php tool/publish.php",
		"test": "phpunit --configuration=etc/phpunit.xml"
	},
	"support": {
		"docs": "https://cedx.github.io/free-mobile.php",
		"issues": "https://github.com/cedx/free-mobile.php/issues"
	}
}
