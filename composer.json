{
	"abandoned": true,
	"description": "Send SMS messages to your Free Mobile device. A simple and free way to monitor your applications using a Free Mobile account.",
	"homepage": "https://bitbucket.org/cedx/free-mobile.php",
	"license": "MIT",
	"name": "cedx/free-mobile",
	"type": "library",
	"version": "16.0.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"psr-4": {"FreeMobile\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"FreeMobile\\": "test/"}
	},
	"config": {
		"optimize-autoloader": true,
		"platform-check": false
	},
	"funding": [
		{"type": "github", "url": "https://github.com/sponsors/cedx"}
	],
	"keywords": [
		"api",
		"client",
		"free",
		"mobile",
		"sdk",
		"sms"
	],
	"require": {
		"php": ">=7.4.0",
		"ext-mbstring": "*",
		"nyholm/psr7": "^1.4.1",
		"psr/http-client": "^1.0.1",
		"symfony/event-dispatcher": "^5.3.7",
		"symfony/http-client": "^5.3.10"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5.10"
	},
	"scripts": {
		"clean": "pwsh tool/clean.ps1",
		"doc": ["@version", "pwsh tool/doc.ps1"],
		"publish": ["@clean", "@version", "pwsh tool/publish.ps1"],
		"test": "phpunit --configuration=etc/phpunit.xml",
		"version": "pwsh tool/version.ps1"
	},
	"support": {
		"docs": "https://bitbucket.org/cedx/free-mobile.php/wiki",
		"issues": "https://bitbucket.org/cedx/free-mobile.php/issues"
	}
}
