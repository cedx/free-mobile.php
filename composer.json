{
	"description": "Send SMS messages to your Free Mobile device.",
	"homepage": "https://github.com/cedx/free-mobile.php",
	"license": "MIT",
	"name": "cedx/free-mobile",
	"type": "library",
	"version": "17.0.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"psr-4": {"FreeMobile\\": "lib/"}
	},
	"autoload-dev": {
		"psr-4": {"FreeMobile\\": "test/"}
	},
	"config": {
		"optimize-autoloader": true,
		"platform-check": false
	},
	"funding": [
		{"type": "github", "url": "https://github.com/sponsors/cedx"}
	],
	"keywords": [
		"api",
		"client",
		"free",
		"mobile",
		"sms"
	],
	"require": {
		"php": ">=8.1.0",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"nyholm/psr7": "^1.5.0",
		"psr/http-client": "^1.0.1",
		"symfony/http-client": "^6.1.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.7.15",
		"phpunit/phpunit": "^9.5.21"
	},
	"scripts": {
		"test": "phpunit --configuration=etc/phpunit.xml"
	},
	"support": {
		"docs": "https://github.com/cedx/free-mobile.php/wiki",
		"issues": "https://github.com/cedx/free-mobile.php/issues"
	}
