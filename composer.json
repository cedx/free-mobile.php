{
  "description": "Send SMS messages to your Free Mobile device. A simple and free way to monitor your applications using a Free Mobile account.",
  "homepage": "https://docs.belin.io/free-mobile.php",
  "license": "MIT",
  "name": "cedx/free-mobile",
  "type": "library",
  "version": "15.1.0",
  "authors": [
    {"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
  ],
  "autoload": {
    "psr-4": {"FreeMobile\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"FreeMobile\\": "test/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "funding": [
    {"type": "patreon", "url": "https://www.patreon.com/cedx"}
  ],
  "keywords": [
    "api",
    "client",
    "free",
    "mobile",
    "sdk",
    "sms"
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "nyholm/psr7": "^1.2.1",
    "psr/http-client": "^1.0.0",
    "symfony/event-dispatcher": "^5.0.8",
    "symfony/http-client": "^5.0.8"
  },
  "require-dev": {
    "cedx/coveralls": "^13.0.0",
    "phpstan/phpstan": "^0.12.25",
    "phpunit/phpunit": "^9.1.4"
  },
  "scripts": {
    "coverage": "coveralls var/coverage.xml",
    "test": "phpunit --configuration=etc/phpunit.xml"
  },
  "support": {
    "docs": "https://api.belin.io/free-mobile.php",
    "issues": "https://git.belin.io/cedx/free-mobile.php/issues"
  }
}
